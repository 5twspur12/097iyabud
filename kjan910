    if bnt == false then
        TextLabel_12.Text = "On"
        TextLabel_12.BackgroundColor3 = Color3.fromRGB(0,255,0)
        bnt = true
    else
        TextLabel_12.Text = "Off"
        TextLabel_12.BackgroundColor3 = Color3.fromRGB(255,0,0)
        bnt = false

    end
end)

