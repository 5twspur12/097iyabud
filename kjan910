        switch = true
        local gato = table.find(blacklistNames, "Gator")
        if gato then
            table.remove(blacklistNames, gato)
        end
    else
        TextLabel_16.Text = "Off"
        TextLabel_16.BackgroundColor3 = Color3.fromRGB(255,0,0)
        switch = false
        table.insert(blacklistNames, "Gator")
    end
end)

